    <script>
        function prev(page, totalPage) {
            page = parseInt(page);
            if (page > 1) {
                window.location.href = `/admin-orders?page=${page - 1}`;
            }
        }
    
        function next(page, totalPage) {
            console.log('next');
            page = parseInt(page);
            totalPage = parseInt(totalPage);
            if (page < totalPage) {
                window.location.href = `/admin-orders?page=${page + 1}`;
            }
        }
    </script>
