        function blockUnblockUser(id, clicked, name) {
    // Display a confirmation message
    Swal.fire({
        title: 'Are you sure?',
        text:` You are about to ${clicked === 'block' ? 'block' : 'unblock'} ${name}?`,
        icon: 'warning',
        showCancelButton: true,
        confirmButtonColor: '#3085d6',
        cancelButtonColor: '#d33',
        confirmButtonText:` Yes, ${clicked}!`
    }).then((result) => {
        if (result.isConfirmed) {
            // User clicked "Yes, block!" - proceed with blocking
            $.ajax({
                url: `/blockuser?id=${id}`,
                method: 'PATCH',
                contentType: 'application/json',
                success: function (data) {
                    if (data.success) {
                        // Display success message after blocking
                        Swal.fire({
                            title:` User ${clicked}ed!`,
                            text: `${name} has been ${clicked}ed successfully.`,
                            icon: 'success',
                        }).then(() => {
                            // Reload the page
                            location.reload();
                        });
                    }
                }
            });
        }
    });
}
    
