    <script>
    function prev(page, totalPage) {
        page = parseInt(page);
        if (page > 1) {
            window.location.href = `/product-list?page=${page - 1}`;
        }
    }

    function next(page, totalPage) {
        console.log('next');
        page = parseInt(page);
        totalPage = parseInt(totalPage);
        if (page < totalPage) {
            window.location.href = `/product-list?page=${page + 1}`;
        }
    }
</script>


    <script>

        const changeProductStatus = (productId) => {
            console.log("clicked", productId);
            Swal.fire({
                title: 'Are you sure?',
                text: 'Do you want to do this?',
                icon: 'warning',
                showCancelButton: true,
                confirmButtonText: 'Yes !',
                cancelButtonText: 'Cancel',
                reverseButtons: true,
            }).then((response) => {
                console.log(response);
                if (response.isConfirmed) {
                    $.ajax({
                        url: "/deleteproduct/" + productId,
                        type: 'PATCH',
                        success:function(response){
                            if(response.success){
                                location.reload()
                            }
                        }
                    })
                    //     Swal.fire("Successfull", `Status Updated`, "success").then(() => {
                    //         location.reload();
                    //     })
                    // }).fail((error) => {
                    //     Swal.fire("Successfull", `${error.message}`, "success").then(() => {
                    //         location.reload()
                    //     })
                    // })
                }
            })



        }
    </script>
